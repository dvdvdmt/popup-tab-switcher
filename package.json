{
  "name": "popup-tab-switcher",
  "version": "1.0.0",
  "description": "A browser extension that makes switching between tabs more convenient.",
  "repository": {
    "type": "git",
    "url": "https://github.com/dvdvdmt/popup-tab-switcher.git"
  },
  "main": "index.js",
  "scripts": {
    "build-prod": "rm -rf build-prod && webpack --env.production && cd build-prod/ && zip -r archive.zip .",
    "build-e2e": "rm -rf build-e2e && webpack --env.e2e",
    "build-dev": "rm -rf build-dev && webpack --env.development",
    "start": "rm -rf build-dev && webpack --env.development --env.watch --watch",
    "test": "mocha --require @babel/register -b ./**/*.unit.test.js",
    "test-e2e": "npm run build-e2e && mocha --require @babel/register -b ./e2e/*.e2e.test.js",
    "clean": "rm -rf build*",
    "lint": "eslint"
  },
  "keywords": [
    "extension",
    "tabs",
    "switching"
  ],
  "author": "Dmitriy Davydov",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.1",
    "deepmerge": "^3.3.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.1.4",
    "node-sass": "^4.12.0",
    "puppeteer": "^1.14.0",
    "raw-loader": "^2.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^4.1.6",
    "svgo": "^1.2.2",
    "svgo-loader": "^2.2.0",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webextension-polyfill-ts": "^0.9.0",
    "webpack": "^4.30.0",
    "webpack-chrome-extension-reloader": "^1.3.0",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "@webcomponents/custom-elements": "^1.2.4",
    "material-components-web": "^2.3.0",
    "svg-baker-runtime": "latest",
    "vue": "^2.6.10",
    "webextension-polyfill": "^0.4.0"
  }
}
